      <property name="forceBigDecimals" value="false"/>
    </javaTypeResolver>

    <javaModelGenerator targetPackage="com.snow.storeapi.entity" targetProject="src/main/java"/>
    <sqlMapGenerator targetPackage="com.snow.storeapi.dao" targetProject="src/main/resources"/>
    <javaClientGenerator type="XMLMAPPER" targetPackage="com.snow.storeapi.dao"
                         targetProject="src/main/java"/>


    <table tableName="vip_info" domainObjectName="Vip" enableCountByExample="false"
           enableUpdateByExample="false" enableDeleteByExample="false"
           selectByExampleQueryId="false" enableSelectByExample="false">
